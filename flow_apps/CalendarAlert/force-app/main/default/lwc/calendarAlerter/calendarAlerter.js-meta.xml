            <property name="firstAlarm" type="Integer"  />
            <property name="secondAlarm" type="Integer"  />
            <property name="thirdAlarm" type="Integer"  />
            <property name="eventList" type="apex://GoogleCalendarEvent[]"  role="inputOnly"/>
            <property name="meetitngRange" type="Integer"  />
            <property name="evaluationFrequency" type="Integer"  />
            <property name="responseStatus" type="Boolean"  />
            <property name="errorText" type="String"  />

        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>